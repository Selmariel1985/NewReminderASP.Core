@model NewReminderASP.Domain.Entities.EventRecurrence

@{
    ViewBag.Title = "Event Recurrence";
}

<link href="~/styles.css" rel="stylesheet" />


<h2>Event Recurrence</h2>

<div class="details-details">
    <h4>Event Recurrence: @Model.RecurrenceType</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>@Html.DisplayNameFor(model => model.RecurrenceType)</dt>
        <dd>@Html.DisplayFor(model => model.RecurrenceType)</dd>


    </dl>


    <div class="btn-group" role="group" aria-label="User actions">
        @Html.ActionLink("Edit", "EditEventRecurrences", new { id = Model.ID }, new { @class = "btn " })
        @Html.ActionLink("Delete", "DeleteEventRecurrence", new { id = Model.ID }, new { @class = "btn " })
        @Html.ActionLink("Back to List", "GetEventRecurrences", null, new { @class = "btn " })
    </div>
</div>
